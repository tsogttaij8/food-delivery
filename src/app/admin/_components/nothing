// –ë“Ø—Ö —Ä–∞–¥–∏–æ button-—É—É–¥—ã–≥ –æ–ª–æ—Ö
const radioButtons = document.querySelectorAll('div[role="radio"]');

// –ë“Ø—Ä—ç–Ω —Å–∞–Ω–∞–ª –Ω–∏–π–ª–Ω—ç –≥—ç–∂ —Å–æ–Ω–≥–æ–≥–¥—Å–æ–Ω —Ç–æ–≤—á–∏–π–≥ –æ–ª–∂ —Ç–æ–≤—à–∏—Ö
radioButtons.forEach(radio => {
    if (radio.getAttribute('aria-label')?.includes("–ë“Ø—Ä—ç–Ω —Å–∞–Ω–∞–ª –Ω–∏–π–ª–Ω—ç")) {
        radio.click(); // click event trigger
        console.log('Clicked:', radio.getAttribute('aria-label'));
    }
});



function rand(n) { 
    return Math.floor(Math.random() * n); 
}

// Multiple choice (radio) —Å–æ–Ω–≥–æ—Ö
document.querySelectorAll(".docssharedWizToggleLabeledContainer").forEach(q => {
    const choices = q.querySelectorAll("div[role='radio']");
    if (choices.length > 0) {
        choices[rand(choices.length)].click();
    }
});

// Checkbox random —Ç“Ø–º—ç–Ω —Å–æ–Ω–≥–æ–ª—Ç
document.querySelectorAll("div[role='checkbox']").forEach(ch => {
    // 50% –º–∞–≥–∞–¥–ª–∞–ª—Ç–∞–π random check/uncheck
    if (Math.random() > 0.5) ch.click();
});

// Dropdown random —Å–æ–Ω–≥–æ–ª—Ç
document.querySelectorAll("div[role='listbox']").forEach(dd => {
    dd.click();
    setTimeout(() => {
        const opts = dd.querySelectorAll("div[role='option']");
        if (opts.length > 0) {
            opts[rand(opts.length)].click();
        }
    }, 200); // 200ms –±–∞–≥–∞ —Ö—É–≥–∞—Ü–∞–∞ —Ç–æ—Ö–∏—Ä—É—É–ª–∂ –±–æ–ª–Ω–æ
});

console.log("üéâ RANDOM –±”©–≥–ª”©–ª—Ç –¥—É—É—Å–ª–∞–∞!");
